                Screen_SignIn.prototype.render = function () {
                    var _this = this;
                    var me = this;
                    GigyaLastPostFormMsg == null; //Reset the last message from gigya form actions;
                    if (typeof grecaptcha !== 'undefined') {
                        LoadCaptcha("signUpCaptchaCon");
                    }
