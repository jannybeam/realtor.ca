(function (Controls) {
    var Desktop;
    (function (Desktop) {
        var DDFFAQ = /** @class */ (function (_super) {
            __extends(DDFFAQ, _super);
            function DDFFAQ(model, controlId) {
                var _this = _super.call(this, model, controlId) || this;
