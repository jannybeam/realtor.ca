        ajaxArgs.callHandler = function (result) {
            handler(result);
        };
        TemplateFetcher.fetch("/Services/TemplateFetcher.asmx/GetRealtorCardTemplate", ajaxArgs);
    };
    TemplateFetcher.fetchSavedSearch = function (handler) {
