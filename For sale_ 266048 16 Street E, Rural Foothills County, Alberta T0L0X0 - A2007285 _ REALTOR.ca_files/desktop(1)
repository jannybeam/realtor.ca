                        args.WorkingRealtorName = Utilities.cleanJSONValue(EmailHelper.getWorkingRealtorName());
                        args.HasBeenPreApprovedMortgage = Utilities.cleanJSONValue(($("#chkMortgagePreApproved").is(":checked") ? "true" : "false"));
