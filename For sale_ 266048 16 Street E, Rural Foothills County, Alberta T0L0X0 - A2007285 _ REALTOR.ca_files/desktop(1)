                    me.closePromoCarousel();
                });
                $(document).keydown(function (e) {
                    if ((e.which && e.which == 13) || (e.keyCode && e.keyCode == 13)) {
                        return false;
                    }
                    else if ((e.which && e.which == 27) || (e.keyCode && e.keyCode == 27)) {
                        me.closePromoCarousel();
                        return false;
                    }
                    else {
                        return true;
                    }
                });
