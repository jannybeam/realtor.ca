(function (Desktop) {
    var Pages;
    (function (Pages) {
        var RealtorSearch = /** @class */ (function (_super) {
            __extends(RealtorSearch, _super);
            function RealtorSearch(model, controlId, reatorSearchVideoIntroUrl, view) {
                var _this = _super.call(this, model, controlId) || this;
