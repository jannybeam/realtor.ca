                });
                Events.Listen(SavedSearch.savedSearchesUpdated, function (arg) {
                    if (SavedSearch.detectedSavedSearchesDiff(me.savedSearches, SavedSearch.GetAll())) {
                        me.RefreshUI();
                    }
                });
                SavedSearch.consumerSavedSearchesSuccess.Listen(function (e) {
