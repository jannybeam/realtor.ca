                                }
                                else {
                                    $("#signInAll .signInTabHRHeading").hide();
                                }
                            }
                            $("#signInPrevious").show();
                        }
                        else {
                            Screen_SignIn.showSignInStandard();
                        }
                    }
                    if (URLHash.get("section") == "SignIn") {
                        me.signInHash();
                    }
                    else if (URLHash.get("section") == "SignUp") {
                        me.signUpHash();
                        $("#signInCreateAccount").click();
                    }
                    else {
                        me.signInHash();
                    }
                };
